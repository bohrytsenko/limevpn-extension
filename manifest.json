 {
    "name": "LimeVPN",
    "version": "1.0",
    "description": "Google Chrome Extension",
    "manifest_version": 2,

    "content_scripts": [
       {
         "matches": [
           "<all_urls>"
         ],
         "js": ["jquery-3.5.1.min.js", "content.js"]
       }
    ],

     "browser_action": {
       "default_icon": "icon.png"
     },

     "background": {
       "scripts": ["background.js"]
     }
  }